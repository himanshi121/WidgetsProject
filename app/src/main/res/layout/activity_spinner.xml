<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relativeLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#B5C7A0DD"
    tools:context=".SpinnerActivity">

    <Spinner
        android:id="@+id/spinner"
        android:layout_width="335dp"
        android:layout_height="40dp"
        android:background="#8B2A043A"
        android:backgroundTint="#DAA57CD3"
        android:dropDownWidth="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.552"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.328"
        tools:layout_conversion_wrapHeight="105"
        tools:layout_conversion_wrapWidth="906"
        />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="326dp"
        android:layout_height="98dp"
        android:text="SPINNER"
        android:textAlignment="center"
        android:textColor="#D324043A"
        android:textSize="60sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.123"
        tools:layout_conversion_wrapHeight="394"
        tools:layout_conversion_wrapWidth="864" />

</androidx.constraintlayout.widget.ConstraintLayout>